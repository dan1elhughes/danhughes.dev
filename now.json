{
  "version": 2,
  "name": "danhughes.dev",
  "alias": ["danhughes.dev", "www.danhughes.dev"],
  "github": {
    "silent": true
  },
  "routes": [
    {
      "src": "/healthz",
      "status": 200
    },
    {
      "src": "/.well-known/carddav",
      "status": 301,
      "headers": { "Location": "https://carddav.fastmail.com" }
    },
    {
      "src": "/.well-known/caldav",
      "status": 301,
      "headers": { "Location": "https://caldav.fastmail.com" }
    },
    {
      "src": "/",
      "status": 302,
      "headers": { "Location": "https://github.com/dan1elhughes" }
    },
    {
      "src": "/keys",
      "status": 302,
      "headers": { "Location": "https://github.com/dan1elhughes.keys" }
    },
    {
      "src": "/(.*)",
      "status": 302,
      "headers": { "Location": "https://github.com/dan1elhughes/$1" }
    }
  ]
}

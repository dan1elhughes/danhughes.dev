{
  "version": 2,
  "name": "danhughes.dev",
  "alias": ["danhughes.dev", "www.danhughes.dev"],
  "github": {
    "silent": true
  },
  "routes": [
    {
      "src": "/healthz",
      "status": 200
    },
    {
      "src": "/",
      "status": 302,
      "headers": { "Location": "https://github.com/dan1elhughes" }
    },
    {
      "src": "/keys",
      "status": 302,
      "headers": { "Location": "https://github.com/dan1elhughes.keys" }
    },
    {
      "src": "/.well-known/webfinger",
      "status": 302,
      "headers": {
        "Location": "https://hachyderm.io/.well-known/webfinger?resource=acct:dan1elhughes@hachyderm.io"
      }
    },
    {
      "src": "/(.*)",
      "status": 302,
      "headers": { "Location": "https://github.com/dan1elhughes/$1" }
    }
  ]
}
